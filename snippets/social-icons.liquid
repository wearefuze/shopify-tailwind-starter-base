<div class="twcss-flex twcss-gap-4 twcss-justify-start twcss-items-center">
  {%- if settings.social_facebook_link != blank -%}
    <div class="twcss-w-8 twcss-h-8 twcss-flex twcss-items-center twcss-justify-center">
      <a href="{{ settings.social_facebook_link }}" class="twcss-text-core-black">
        {%- render 'icon-facebook' -%}
        <span class="twcss-sr-only">{{ 'general.social.links.facebook' | t }}</span>
      </a>
    </div>
  {%- endif -%}
